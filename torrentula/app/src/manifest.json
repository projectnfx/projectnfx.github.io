{
  "name": "Torrentula",
  "description": "Download everything with webtorrent!",
  "version": "1",
  "manifest_version": 2,
  "icons": {
    "16": "img/icons/icon16.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "key": "",
  "app": {
    "background": {
      "scripts": ["js/background.js"],
      "persistent": true
    }
  },
  "permissions": [
    "http://localhost:35729/",
    "http://*/",
    "https://*/",
    "storage",
    "unlimitedStorage",
    {"fileSystem": ["write"]}
  ]
}
